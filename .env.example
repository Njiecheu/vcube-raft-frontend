# Configuration de l'API Backend - Multi-nœuds
VITE_API_BASE_URL=http://localhost:8080
VITE_API_BACKUP_URLS=http://localhost:8081,http://localhost:8082

# Configuration alternative - Liste complète des nœuds
VITE_API_NODES=http://localhost:8080,http://localhost:8081,http://localhost:8082

# Configuration WebSocket pour les mises à jour temps réel
VITE_WS_URL=ws://localhost:8080/ws
VITE_WS_BACKUP_URLS=ws://localhost:8081/ws,ws://localhost:8082/ws

# Configuration pour les tests de performance
VITE_ENABLE_PERFORMANCE_TESTING=true

# Configuration de logging
VITE_LOG_LEVEL=development
